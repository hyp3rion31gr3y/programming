import random
dict = {}

def read(n):
    with open(n, encoding="utf-8") as f:
        t = f.read()
        a = t.split('\n')
    return a

def splitt(r, dict):
    for i in r:
        a = i.split(',')
        #print(a[0])
        dict[a[0]] = a[1:]
        #print(dict[a[0]])
    return dict

